%% Logtalk definition (c) 2012 Paulo Moura
%%
\lst@definelanguage{Logtalk}{%
	tabsize=4,%
	upquote=true,%
	morekeywords={},%
	otherkeywords={::,\^\^,\{,\}},%
	moredirectives={
		object,end_object,protocol,end_protocol,category,end_category,%
		public,protected,private,%
		alias,dynamic,discontiguous,synchronized,multifile,%
		annotation,%
		meta_predicate,mete_non_terminal,%
		initialization,set_logtalk_flag,%
		info,mode,%
		if,elif,else,endif%
	},%
	moredelim=*[directive]:-,%
	sensitive=true,%
	morecomment=[l]\%,%
	morecomment=[s]{/*}{*/},%
	morestring=[bd]",%
	morestring=[bd]'%
}[keywords,comments,strings,directives]%
\endinput
